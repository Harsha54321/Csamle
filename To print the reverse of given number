// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n, t, r=0;
    printf("\n Enter any number to reverse");
    scanf ("%d",&n);
    printf ("\n n= %d",n);
    while(n>0){
        t=n%10;
        r=(r*10)+t;
        n=n/10;
    }
    printf ("\n Reverse of the given number=%d",r);
    
    return 0;
}
