// Online C compiler to run C program online
#include <stdio.h>
void swap (int, int);
int main() {
    // Write C code here
    int x, y;
    printf("\nenter two values");
    scanf ("%d%d",&x,&y);
    printf ("\nbefore swapping x=%d,y=%d",x,y);
    swap(x,y);
    printf ("\nafter swapping x=%d,y=%d",x,y);
    
    return 0;
}
void swap (int a,int b){
    int temp;
    temp=b;
    b=a;
    a=temp;
    printf ("\nvalues of a=%d b=%d",a,b);
}

