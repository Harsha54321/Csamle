// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int r=2,n,t, x;
    printf ("\nEnter any number");
    scanf ("%d",&n);
    printf ("\n n=%d",n);
    x=n;
    do{
        t=n%10;
        r=(r*10)+t;
        n=n/10;
    }while(n>0);
    if(r==x)
        printf ("\n %d is palindrome",x);
    else
        printf ("\n %d is not palindrome",x);
    
    return 0;
}
