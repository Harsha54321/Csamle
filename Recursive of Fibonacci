// Online C compiler to run C program online
#include <stdio.h>
int fibo(int);
int main() {
    // Write C code here
    int n, f;
    printf("\nenter any number");
    scanf ("%d",&n);
    f=fibo(n);
    printf ("%d",f);
}
int fibo(int n){
    if(n==0||n==1)
        return 1;
    else
        return fibo(n-1)+fibo(n-2);
}
