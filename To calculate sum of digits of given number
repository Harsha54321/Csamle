// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n, t, sum=0;
    printf("\n Enter any number to calculate sum of digits of given number");
    scanf ("%d",&n);
    printf ("\n n= %d",n);
    while(n>0){
        t=n%10;
        sum=(t)+sum;
        n=n/10;
    }
    printf ("\n sum of digits=%d",sum);
    
    return 0;
}
