// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int i,num,n,big;
    big=0;
    printf("enter value of n:");
    scanf ("%d",&n);
    printf("\n %d",n);
    printf ("\n %d",big);
    for(i=1;i<=n;i++){
        printf("\n enter number%d:",i);
        scanf ("%d",&num);
        printf ("\n %d",num);
        if(num>big)
        big=num;
    }
    printf ("\n%dis biggest number among n numbers",big);  
    return 0;
}
