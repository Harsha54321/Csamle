int main() {
    // Write C code here
    int x, y, z;
    printf("\n Enter three different numbers to test the biggest number among three numbers");
    scanf("%d%d%d",&x,&y,&z);
    printf ("\n x=%d,y=%d,z=%d",x,y,z);
