// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int i,j, n, ctr;
    printf("\n Enter any number");
    scanf ("%d",&n);
    printf ("\n prime numbers are:");
    for (i=2;i<=n;i++){
        ctr=0;
        for (j=1;j<=i;j++){
            if(i%j==0){
                ctr++;
            }
        }
        if(ctr==2){
            printf ("%d",i);
        }
    }
    
    return 0;
}
