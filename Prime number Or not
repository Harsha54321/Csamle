// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int i,n,ctr=0;
    printf("Enter any number");
    scanf ("%d",&n);
    printf ("\n%d",n);
    for (i=2;i<n;i++){
        if(n%i==0)
            ctr++;
    }
    if(ctr==0)
        printf ("\n %d is a prime number",n);
    else
        printf ("\n %d is not a prime number",n);
    
    return 0;
}
