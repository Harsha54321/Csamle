// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int i,num,n,small;
    small='INT_MAX';
    printf("enter value of n:");
    scanf ("%d",&n);
    printf("\n %d",n);
    printf ("\n %d",small);
    for(i=1;i<=n;i++){
        printf("\n enter number%d:",i);
        scanf ("%d",&num);
        printf ("\n %d",num);
        if(num<small)
        small=num;
    }
    printf ("\n%dis smallest number among n numbers",small);  
    return 0;
}
