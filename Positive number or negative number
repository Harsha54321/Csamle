// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n;
    printf("\n Enter any number to test whether it is positive number or negative number");
    scanf("%d",&n);
    printf ("\n n=%d",n);
    n>0? printf ("\n %d is positive number",n):printf ("\n %d is negative number",n);
    
    return 0;
}
