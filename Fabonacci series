// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int a=0,b=1,c,i,n;
    printf("Enter how many numbers you want to print in fabonacci series");
    scanf ("%d",&n);
    printf ("\n%d",n);
    for (i=1;i<=n;i++){
        c=a+b;
        printf ("\n %d",a);
        a=b;
        b=c;
    }
    
    return 0;
}
