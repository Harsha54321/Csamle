// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n, t, r=0,x;
    printf("\n Enter any number to find whether it is palindrome or not");
    scanf ("%d",&n);
    printf ("\n n= %d",n);
    x=n;
    while(n>0){
        t=n%10;
        r=(r*10)+t;
        n=n/10;
    }
    if(r==x)
        printf ("\n %d is palindrome",x);
    else
        printf ("\n %d is not palindrome",x);
    
    return 0;
}
