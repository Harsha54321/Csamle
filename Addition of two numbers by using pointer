// Online C compiler to run C program online
#include <stdio.h>

void main() {
    // Write C code here
    int a=2,b=7,*p1,*p2;
    printf("\nvalue of a=%d",a);
    printf ("\nvalue of b=%d",b);
    p1=&a;
    p2=&b;
    printf ("\naddress of a=%u",p1);
    printf ("\naddress of b=%u",p2);
    printf ("\naddition of a&b=%d",*p1+*p2);
    
    return 0;
}
