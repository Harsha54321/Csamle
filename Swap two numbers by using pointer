// Online C compiler to run C program online
#include <stdio.h>

void main() {
    // Write C code here
    int a=4, b=3, c, *p1, *p2, *p3;
    p1=&a;
    p2=&b;
    p3=&c;
    *p3=*p1;
    *p1=*p2;
    *p2=*p3;
    printf("\nafter swap a=%d",a);
    printf ("\nafter swap b=%d",b);
    
    return 0;
}
